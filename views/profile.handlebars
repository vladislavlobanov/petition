<div class="frameHolder">

    <div class="insideframeHolder">
        <div class="frame">
            <form class="frameContainer" method="POST">

                <h1 class="regEl">TELL US ABOUT YOURESELF</h1>
                <div class="bannerRegister">#FREENAVALNY</div>

                <div class="regEl">

                    {{>error status=smthWrong}} 
                    <div class="frameContainer center">
                        <div>
                            <label for="ageId">Age:</label>
                            <input
                                type="number"
                                id="ageId"
                                name="age"
                                min="18"
                                max="100"
                            /></div>

                        <div>
                            <label for="cityId">City:</label>
                            <input type="text" id="cityId" name="city" /></div>

                        <div>
                            <label for="homepageId">Homepage:</label>

                            <input
                                type="text"
                                pattern="https?://.+"
                                id="homapageId"
                                name="homepage"
                                placeholder="http://example.com"
                            />

                        </div>

                        <input
                            type="hidden"
                            name="_csrf"
                            value="{{csrfToken}}"
                        />
                        <div class="wrapper"><button>CONTINUE</button></div>
                    </div>
                </div>
            </form>
        </div></div>

</div>